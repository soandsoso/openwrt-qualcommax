#!/bin/sh

[ -e /lib/firmware/$FIRMWARE ] && exit 0

. /lib/functions/caldata.sh

board=$(board_name)

case "$FIRMWARE" in
"ath11k/IPQ6018/hw1.0/cal-ahb-c000000.wifi.bin")
	case $(board_name) in
	cmiot,ax18|\
	qihoo,v6|\
	zn,m2|\
	glinet,gl-axt1800|\
	glinet,gl-ax1800|\
	redmi,ax5|\
	xiaomi,ax1800ï¼‰
		caldata_extract "0:art" 0x1000 0x10000
		;;
	redmi,ax5-jdcloud|\
	jdc,ax1800-pro)
		caldata_extract_mmc "0:ART" 0x1000 0x10000
		;;
	*)
		exit 1
		;;
	esac
esac
